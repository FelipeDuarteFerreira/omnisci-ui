    "react-dom": "16.8.3",
    "react-scripts": "2.1.5",
    "react-styleguidist": "9.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "tslint-react": "^3.6.0",
    "typescript": "3.3.3333"
  },
  "devDependencies": {
