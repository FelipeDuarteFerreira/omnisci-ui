{
  "name": "omnisci-ui",
  "version": "0.1.0",
  "description": "OmniSci component ui library",
  "author": "OmniSci",
  "main": "dist/omnisci-ui.js",
  "contributors": [
    {
      "name": "Christophe Viau",
      "email": "christopheviau@gmail.com"
    }
  ],
  "scripts": {
    "build": "yarn build:prod && yarn build:dev",
    "build:prod": "webpack --env.prod",
    "build:dev": "webpack --env.dev",
    "prod": "webpack --env.prod --watch",
    "dev": "webpack --env.dev --watch",
    "clean": "yarn cache clean && rm -rf node_modules",
    "test": "jest --config jest.config.json",
    "test:snapshot": "jest --config jest.config.json --updateSnapshot",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omnisci/omnisci-ui"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/omnisci/omnisci-ui/issues"
  },
  "homepage": "https://github.com/omnisci/omnisci-ui",
  "browserslist": [
    "last 2 versions",
    "not ie 10"
  ],
  "dependencies": {
    "@babel/cli": "^7.2.3",
