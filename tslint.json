{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "quotemark": [true, "double"],
    "ordered-imports": false,
    "trailing-comma": true,
    "semicolon": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "jsx-no-multiline-js": false
  }
}